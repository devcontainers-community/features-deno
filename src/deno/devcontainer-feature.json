{
    "name": "Deno",
    "id": "deno",
    "version": "1.2.1",
    "description": "Installs Deno",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "none",
                "1.41.0",
                "1.40.5",
                "1.39.4",
                "1.38.5",
                "1.37.2",
                "1.36.4",
                "1.35.3",
                "1.34.3",
                "1.33.4",
                "1.32.5",
                "1.31.3",
                "1.30.3",
                "1.29.4",
                "1.28.3",
                "1.27.2",
                "1.26.2",
                "1.25.4",
                "1.24.3"
            ],
            "default": "latest",
            "description": "Select or enter a Deno version to install"
        }
    },
    "customizations": {
        "vscode": {
            "settings": { 
				"deno.enable": true,
				"deno.lint": true,
				"editor.defaultFormatter": "denoland.vscode-deno"
			},
            "extensions": [
                "denoland.vscode-deno"
            ]
        }
    }
}
