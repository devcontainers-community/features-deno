{
    "name": "Deno",
    "id": "deno",
    "version": "1.2.2",
    "description": "Installs Deno",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "none",
                "2.5.6",
                "2.6.1"
            ],
            "default": "latest",
            "description": "Select or enter a Deno version to install"
        }
    },
    "customizations": {
        "vscode": {
            "settings": {
                "deno.enable": true,
                "deno.lint": true,
                "editor.defaultFormatter": "denoland.vscode-deno"
            },
            "extensions": [
                "denoland.vscode-deno"
            ]
        }
    }
}
